    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.11.0",
    "babel-register": "^6.11.6",
    "eslint": "4.19.1",
    "flow-bin": "0.74.0",
    "nodemon": "^1.10.0",
    "prettier": "1.13.7"
  },
  "dependencies": {
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2",
